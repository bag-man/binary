{
  "compilerOptions": {
    "outDir": "./build/",
    "rootDir": "./src/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "lib": [ "es2017", "esnext.asynciterable" ],
    "moduleResolution": "node",
    "strict": true,
    "noImplicitReturns": true,
    "typeRoots": [ "./types", "./node_modules/@types"]
  },
  "include": [ "src", "./types" ]
}
